{
    "name": "flywaytasks",
    "version": "1.0.0",
    "scripts": {
        "compile:tasks": "glob-exec \"tasks/*/tsconfig.json\" -- \"tsc -b {{files.join(' ')}}\"",
        "package:tasks": "tfx extension create --manifest-globs vss-extension.json",
        "package": "glob-exec --parallel --foreach \"tasks/*/tsconfig.json\" -- \"cd {{file.dir}} && npm dedupe && npm prune --omit=dev\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "glob-exec": "^0.1.1"
    }
}
