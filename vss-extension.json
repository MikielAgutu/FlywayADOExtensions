{
    "manifestVersion": 1,
    "id": "flyway-task",
    "name": "Flyway CLI",
    "version": "0.0.1",
    "publisher": "MikielAgutu",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Run Flyway CLI",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/flyway.png"        
    },
    "files": [
        {
